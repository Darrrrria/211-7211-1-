/*// Задача 2
#include <iostream>
using namespace std;
int a, b, i;
int main() {
	setlocale(LC_ALL, "Russian");
	cout << "a>b; a = ";
	cin >> a;
	cout << "a>b; b = ";
	cin >> b;
	i=0;
	while (a>=b){
	    a-=b;
	    i++;
	}
	cout << "Количество отрезков b на отрезке a = " << i;
}*/


/*// Задача 3
